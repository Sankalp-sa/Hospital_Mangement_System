    <div class="menu-bar">
      <div class="menu">
        <ul class="menu-links">
          <li class="nav-link">
            <a href="/DocViewappointment">
              <i class="bx bx-home-alt icon"></i>
              <span class="text nav-text">View Appointment</span>
