<section class="home ">
    <div class=" rounded-lg shadow-md p-8">
        <h1 class="text-3xl font-semibold mb-6">Patient Appointment History</h1>
        <form class="flex items-center">
          <input
            type="text"
            placeholder="Enter Patient Name or ID"
            class="flex-grow py-2 px-4 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-400"
          />
          <button
            type="submit"
            class="bg-blue-500 text-white py-2 px-4 ml-4 rounded-lg hover:bg-blue-600 focus:outline-none focus:ring-2 focus:ring-blue-400"
          >
            Search
          </button>
        </form>
        <div class="mt-8">
          <h2 class="text-xl font-semibold mb-4">Patient Appointments</h2>
          <table class="w-full border-collapse">
            <thead class="rounded-md">
              <tr class="bg-blue-300">
                <th class="py-3 px-6 text-left">Patient Name</th>
                <th class="py-3 px-6 text-left">Appointment Date</th>
                <th class="py-3 px-6 text-left">Appointment Time</th>
              </tr>
            </thead>
            <tbody>
              <tr class="border-t">
                <td class="py-4 px-6 text-left">
                  <div class="flex items-center">
                    <div class="flex-shrink-0">
                      <img
                        src="https://via.placeholder.com/40"
                        alt="Patient Profile Picture"
                        class="w-10 h-10 rounded-full object-cover mr-4"
                      />
                    </div>
                    <div>
                      <h3 class="text-lg font-semibold">John Smith</h3>
                    </div>
                  </div>
                </td>
                <td class="py-4 px-6 text-left">April 5, 2023</td>
                <td class="py-4 px-6 text-left">3:00 PM</td>
              </tr>
              <tr class="border-t">
                <td class="py-4 px-6 text-left">
                  <div class="flex items-center">
                    <div class="flex-shrink-0">
                      <img
                        src="https://via.placeholder.com/40"
                        alt="Patient Profile Picture"
                        class="w-10 h-10 rounded-full object-cover mr-4"
                      />
                    </div>
                    <div>
                      <h3 class="text-lg font-semibold">Jane Doe</h3>
                    </div>
                  </div>
                </td>
                <td class="py-4 px-6 text-left">March 28, 2023</td>
                <td class="py-4 px-6 text-left">10:30 AM</td>
              </tr>
              <tr class="border-t">
                <td class="py-4 px-6 text-left">
                  <div class="flex items-center">
                    <div class="flex-shrink-0">
                      <img
                        src="https://via.placeholder.com/40"
                        alt="Patient Profile Picture"
                        class="w-10 h-10 rounded-full object-cover mr-4"
                      />
                    </div>
                    <div>
                      <h3 class="text-lg font-semibold">Michael Johnson</h3>
                    </div>
                  </div>
                </td>
                <td class="py-4 px-6 text-left">March 20, 2023</td>
                <td class="py-4 px-6 text-left">2:45 PM</td>
              </tr>
              <!-- Additional rows for patient appointments can be added here -->
            </tbody>
          </table>
        </div>
      </div>
      
</section>

 
 
<script src="./js/script123_docIndex.js"></script>

    
